{
  "name": "rental-listings-backend",
  "version": "1.0.0",
  "description": "Backend API for rental listings application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "init-db": "ts-node src/scripts/initDatabase.ts",
    "seed": "ts-node src/scripts/seedData.ts",
    "generate-data": "ts-node src/scripts/generateRealisticData.ts",
    "generate-corrected": "ts-node src/scripts/generateCorrectedData.ts",
    "clear-and-regenerate": "ts-node src/scripts/clearAndRegenerate.ts",
    "regenerate-complete": "ts-node src/scripts/regenerateCompleteData.ts",
    "generate-realistic": "ts-node src/scripts/run-generator.ts",
    "generate-taiwan": "ts-node src/scripts/run-taiwan-generator.ts"
  },
  "keywords": [
    "rental",
    "listings",
    "api",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
